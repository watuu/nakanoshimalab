body#body(class=pageClass)
    = "\n"
    a#top
    .l-body-wrap
        header.l-header
            .l-header__wrap
                p.l-header-logo
                    a(href=path+"")
                        svg(width=195 height=40 role="img" aria-label=sitename)
                            use(href="#logo")
                nav.l-header-nav
                    ul.l-header-nav__nav
                        li
                            button.c-btn-header-about(data-micromodal-trigger="modal-about")
                                span.c-btn-header-about__txt 中之島について
                                i.c-btn-header-about__plus


                    ul.l-header-nav__locale.c-link-local
                        li
                            p.js-split-text JP
                        li
                            a(href="#").js-split-text EN
                button.l-header-menu.c-btn-menu(aria-label="メニューを開く")
                    span Menu
                    span Close
            .l-header-drawer.is-section-dark
                .l-header-drawer__bg
                .l-header-drawer__wrap
                    .l-header-drawer__inner
                        nav.l-header-drawer__nav
                            include ./template/cm-nav-main
                        .l-header-drawer-recommend
                            .l-header-drawer-recommend__head
                                    span Recommend
                                    span おすすめコンテンツ
                            .l-header-drawer-recommend__list
                                - for (var x = 1; x <= 3; x++)
                                    .c-card-recommend
                                        a(href=path+"single-news/").c-card-recommend__link
                                            figure.c-card-recommend__pic
                                                img(src=img + "500x500.webp" alt="")
                                            .c-card-recommend__body
                                                h3.c-card-recommend__title
                                                    | 「中之島15の場所での物語」AR挿絵公募
                                                ul.c-card-recommend__tag
                                                    li #Events
                        //-.l-header-drawer__cta
                            a(href="#").c-btn-ticket
                                span.c-btn-ticket__txt Ticket購入
                                i.c-btn-ticket__arrow
                                    svg(width=26 height=26).js-clone
                                        use(href="#ico-arrow")


        main.l-main#main
            block content
        footer.l-footer
            .l-container.is-section-dark
                .l-footer__wrap
                    .l-footer-nav
                        include ./template/cm-nav-main
